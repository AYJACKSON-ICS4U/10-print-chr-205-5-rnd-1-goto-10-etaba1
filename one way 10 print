var x = 0;
var y = 0; 
var spacing = 20;

draw = function() {
    stroke(0,0,0);
    if (random[2] < 1) {
        line (x, y, x+spacing, y+spacing);
    }
    else{
        line (x, y+spacing, x+spacing, y);
    }
    x = x + spacing;
    if (x > width){
        x = 0;
        y = y + spacing;
    }
        
};
